{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "includeFiles": [
          ".next/**",
          "!.next/cache/**",
          "node_modules/.pnpm/@prisma+client@*/node_modules/@prisma/**",
          "prisma/**",
          "!.env"
        ]
      }
    }
  ],
  "buildCommand": "npx prisma generate && next build"
}